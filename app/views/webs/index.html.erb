<p id="notice"><%= notice %></p>

<h1>Webs</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Phone</th>
      <th>Aboutcompany</th>
      <th>Aboutproject</th>
      <th>Projectdeadline</th>
      <th>Projectbudget</th>
      <th>Targetaudience</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @webs.each do |web| %>
      <tr>
        <td><%= web.name %></td>
        <td><%= web.email %></td>
        <td><%= web.phone %></td>
        <td><%= web.aboutcompany %></td>
        <td><%= web.aboutproject %></td>
        <td><%= web.projectdeadline %></td>
        <td><%= web.projectbudget %></td>
        <td><%= web.targetaudience %></td>
        <td><%= link_to 'Show', web %></td>
        <td><%= link_to 'Edit', edit_web_path(web) %></td>
        <td><%= link_to 'Destroy', web, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Web', new_web_path %>
