<!-- Modal -->
<div class="modal fade" id="exampleModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="exampleModalLabel">Kindly fill this form.</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
               <%= form_for(@dream) do |f| %>
<br>
  <h5 style="text-align: center;">Website & Budget</h5>
<hr>
   <div class="form-group">
    <label>What type of website do you want?</label><br>
        <select class="selecter" name="dream[dreamtype]" id="dream_dreamtype" style="width: 100%;">
          <option name="dream[dreamtype]" id="dream_dreamtype" value="Personal Website">Personal Website</option>
          <option name="dream[dreamtype]" id="dream_dreamtype" value="Portfolio Website">Portfolio Website</option>
          <option name="dream[dreamtype]" id="dream_dreamtype" value="Company Website">Company Website</option>
          <option name="dream[dreamtype]" id="dream_dreamtype" value="Church Website">Church Website</option>
          <option name="dream[dreamtype]" id="dream_dreamtype" value="School Website">School Website</option>
          <option name="dream[dreamtype]" id="dream_dreamtype" value="Business Website">Business Website</option>
          <option name="dream[dreamtype]" id="dream_dreamtype" value="Ecommerce Web Applications">Ecommerce Web Applications</option>
          <option name="dream[dreamtype]" id="dream_dreamtype" value="Job Portal Web Applications">Job Portal Web Applications</option>
          <option name="dream[dreamtype]" id="dream_dreamtype" value="Customized Business Web Applications">Customized Business Web Applications</option>
        </select>
      </div>
       <div class="form-group">
    <label>What is your budget Allocation for your website?</label><br>
        <select class="selecter" name="dream[dreambudget]" id="dream_dreambudget" style="width: 100%;">
          <option name="dream[dreambudget]" id="dream_dreambudget" value="300 - 499">300 - 499 cedis</option>
          <option name="dream[dreambudget]" id="dream_dreambudget" value="500 - 699">500 - 699 cedis</option>
          <option name="dream[dreambudget]" id="dream_dreambudget" value="700 - 999">700 - 999 cedis</option>
          <option name="dream[dreambudget]" id="dream_dreambudget" value="1000 - 1499">1000 - 1499 cedis</option>
          <option name="dream[dreambudget]" id="dream_dreambudget" value="2000 - 2999">2000 - 2999 cedis</option>
          <option name="dream[dreambudget]" id="dream_dreambudget" value="3000 +">3000+ cedis </option>
        </select>
      </div>
      <div class="form-check">
    <label class="form-check-label">
         <%= f.check_box :urgent, :class => "form-check-input" %>
      Check box if urgent
    </label>
  </div>
<br>
  <h5 style="text-align: center;">Your Contact Details</h5>
<hr>
  <div class="form-group">
    <label>Name</label>
    <input type="text" name="dream[dreamername]" id="dream_dreamername" class="form-control" placeholder="Your Name">
  </div>
  <div class="form-group">
    <label>Email</label>
    <input type="email" name="dream[dreameremail]" id="dream_dreameremail" class="form-control" placeholder="Email Address">
  </div>
  <div class="form-group">
    <label>Phone Number</label>
    <input type="number" name="dream[dreamerphone]" id="dream_dreamerphone" class="form-control" placeholder="Your Phone number">
  </div>
 
       <p style="color: #5cc804; text-align: center;">Response time is usually within a day. More information will be requested when we contact you. Thank you.</p> 
        <p style="text-align: center; "><button type="submit" class="btn btn-primary modalsubmit">Submit</button></p>
      </div>
       
      
      
      </div>
     
      <% end %>
