<p id="notice"><%= notice %></p>

<h1>Emailcourses</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Businessname</th>
      <th>Email</th>
      <th>Phonenumber</th>
      <th>Staff</th>
      <th>Resultsiwant</th>
      <th>Qualified status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @emailcourses.each do |emailcourse| %>
      <tr>
        <td><%= emailcourse.name %></td>
        <td><%= emailcourse.businessname %></td>
        <td><%= emailcourse.email %></td>
        <td><%= emailcourse.phonenumber %></td>
        <td><%= emailcourse.staff %></td>
        <td><%= emailcourse.resultsiwant %></td>
        <td><%= emailcourse.qualified %></td>
        <td><%= link_to 'Show', emailcourse %></td>
        <td><%= link_to 'Edit', edit_emailcourse_path(emailcourse) %></td>
        <td><%= link_to 'Destroy', emailcourse, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Emailcourse', new_emailcourse_path %>
